<?xml version="1.0"?>
<extension key="de.systopia.sepacustom" type="module">
  <file>sepacustom</file>
  <name>CiviSEPA Customisations</name>
  <description>Additional Configuration Options for CiviSEPA</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.sepacustom</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.sepacustom</url>
    <url desc="Support">https://github.com/systopia/de.systopia.sepacustom/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.3-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
